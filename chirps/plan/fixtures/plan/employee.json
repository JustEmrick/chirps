      "severity": 2
    },
    "model": "plan.Rule",
    "pk": 104
  },
  {
    "fields": {
      "name": "Salary",
      "plan": 100,
      "query_string": "What's my salary?",
      "regex_test": "\\$\\d+(,\\d{3})*(\\.\\d{2})?",
      "severity": 2
    },
    "model": "plan.Rule",
    "pk": 105
  }
